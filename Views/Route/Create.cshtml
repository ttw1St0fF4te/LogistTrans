@model LogistTrans.Models.Route

<h2>Create Route</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="OrderId" class="control-label"></label>
        <select asp-for="OrderId" asp-items="ViewBag.OrderId" class="form-control"></select>
    </div>
    <div class="form-group">
        <label asp-for="EmployeeId" class="control-label"></label>
        <select asp-for="EmployeeId" asp-items="ViewBag.EmployeeId" class="form-control"></select>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>